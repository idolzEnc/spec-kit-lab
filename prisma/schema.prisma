generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RollAudit {
  id           String   @id @default(uuid())
  expression   String
  mode         String
  rollSets     Json
  selectedIndex Int
  total        Int
  createdAt    DateTime @default(now())
}
